<h2>Posts</h2>
<div class="list">
  <% @category.posts.each do |post| %>
    <li><a class ="category"><%= post.title %></a><br><%= link_to "Show", category_post_path(@category, post)%></li>
  <%end%>
</div>

<%= link_to "Create Post", new_category_post_path(@category) %>
<%= link_to "Back", categories_path%>
